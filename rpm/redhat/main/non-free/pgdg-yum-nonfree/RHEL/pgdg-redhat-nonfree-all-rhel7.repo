
# PGDG Red Hat Enterprise Linux / CentOS non-free stable repositories:

[pgdg15-nonfree]
name=PostgreSQL 15 for RHEL/CentOS $releasever - $basearch - NonFree
baseurl=https://download.postgresql.org/pub/repos/yum/non-free/15/redhat/rhel-$releasever-$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/PGDG-RPM-GPG-KEY-RHEL7-nonfree
repo_gpgcheck = 1

[pgdg14-nonfree]
name=PostgreSQL 14 for RHEL/CentOS $releasever - $basearch - NonFree
