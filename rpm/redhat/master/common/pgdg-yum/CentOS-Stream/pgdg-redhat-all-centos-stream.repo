# PGDG CentOS Stream stable common repository for all PostgreSQL versions

[pgdgcentosstream-common]
name=PostgreSQL common RPMs for CentOS Stream $releasever - $basearch
baseurl=https://download.postgresql.org/pub/repos/yum/stream/common/redhat/rhel-$releasever-$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-PGDG
repo_gpgcheck = 1

